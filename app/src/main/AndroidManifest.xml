<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.delivery">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Delivery"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".PaymentActivity"
            android:exported="false" />
        <activity
            android:name=".CartViewModel"
            android:exported="false" />
        <activity
            android:name=".cartItem"
            android:exported="false" />
        <activity
            android:name=".CartViewModelFactory"
            android:exported="false" />
        <activity
            android:name=".CartViewActivity"
            android:exported="false" />
        <activity
            android:name=".CartFragment"
            android:exported="false" />
        <activity
            android:name=".Gif8Activity"
            android:exported="false" />
        <activity
            android:name=".Gif7Activity"
            android:exported="false" />
        <activity
            android:name=".Gif6Activity"
            android:exported="false" />
        <activity
            android:name=".Gif5Activity"
            android:exported="false" />
        <activity
            android:name=".Gif4Activity"
            android:exported="false" />
        <activity
            android:name=".Gif3Activity"
            android:exported="false" />
        <activity
            android:name=".Gif2Activity"
            android:exported="false" />
        <activity
            android:name=".Gif1Activity"
            android:exported="false" />
        <activity
            android:name=".FontBaseActivity"
            android:exported="false" />
        <activity
            android:name=".SignupActivity"
            android:exported="false"
            tools:ignore="MissingClass" /> <!-- SplashActivity -->
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.Delivery"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            tools:ignore="MissingClass" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            tools:ignore="MissingClass" />
        <activity
            android:name=".AppSettingsActivity"
            android:exported="false"
            tools:ignore="MissingClass" />
        <activity
            android:name=".AccountSettingsActivity"
            android:exported="false"
            tools:ignore="MissingClass" />
    </application>

</manifest>